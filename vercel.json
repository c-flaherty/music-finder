{
    "builds": [
      {
        "src": "frontend/music-finder/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/api/**/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/search-songs",
        "dest": "backend/api/search_songs.py"
      },
      {
        "src": "/api/spotify/(.*)",
        "dest": "frontend/music-finder/api/spotify/$1"
      },
      {
        "src": "/api/auth/(.*)",
        "dest": "frontend/music-finder/api/auth/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/music-finder/$1"
      }
    ]
  }
  